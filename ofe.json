{
  "default": {
    "extensions": [""],
    "files": ["ofe.json", ".bin/*"],
    "exclusions": ["*.ogg"]
  },
  "zsh": {
    "files": ["zsh*", "zshrc-specific-to-*", "etc/zsh/*", "zsh/*"],
    "first_file": "zshrc"
  },
  "tmux": {
    "files": ["tmux.conf"]
  },
  "hyper": {
    "files": ["hyper.js"]
  },
  "vim": {
    "files": [
      "vim/lua/init.lua",
      "vim/lua/**/*.lua"
    ],
    "first_file": "vim/lua/init.lua",
    "exclusions": ["vim/plugged"]
  },
  "ssh": {
    "files": ["ssh/*"]
  },
  "hammerspoon": {
    "files": ["hammerspoon/*"]
  },
  "hs": {
    "files": ["hammerspoon/*"]
  },
  "fluxbox": {
    "files": ["fluxbox/**/**"]
  },
  "alacritty": {
    "files": ["alacritty/*"]
  },
  "diff": {
    "command": "git diff --name-only"
  },
  "git": {
    "files": [".git/config", ".gitignore", "gitconfig"],
    "first_file": "gitconfig"
  }
}
