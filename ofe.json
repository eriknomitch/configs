{
  "default": {
    "extensions": [""],
    "files":      ["ofe.json", ".bin/*"],
    "exclusions": ["*.ogg"]
  },
  "zsh": {
    "files": ["zsh*", "zshrc-specific-to-*"]
  },
  "tmux": {
    "files": ["tmux.conf"]
  },
  "hyper": {
    "files": ["hyper.js"]
  },
  "vim": {
    "files": ["vimrc", "vim/**/**"],
    "first_file": "vimrc"
  },
  "ssh": {
    "files": ["ssh/*"]
  },
  "hammerspoon": {
    "files": ["hammerspoon/*"]
  },
  "hs": {
    "files": ["hammerspoon/*"]
  },
  "fluxbox": {
    "files": ["fluxbox/**/**"]
  },
  "diff": {
    "command": "git diff --name-only"
  },
  "git": {
    "files": [".git/config", ".gitignore", "gitconfig"],
    "first_file": "gitconfig"
  }
}
