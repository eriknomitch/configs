{
  "default": {
    "extensions": [""],
    "files":      ["ofe.json", ".bin/*"]
  },
  "zsh": {
    "files": ["zsh*", "zshrc-*-specific/*"]
  },
  "vim": {
    "files": ["vimrc", "vim/**/**"],
    "first_file": "vimrc"
  },
  "ssh": {
    "files": ["ssh/*"]
  },
  "fluxbox": {
    "files": ["fluxbox/**/**"]
  },
  "diff": {
    "command": "git diff --name-only"
  },
  "git": {
    "files": [".git/config", ".gitignore", "gitconfig"],
    "first_file": "gitconfig"
  }
}
